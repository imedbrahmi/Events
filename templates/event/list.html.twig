<table border="2">
    <tr>
        <td>Date Debut</td>
        <td>Date Fin</td>
        <td>Lieu</td>
        <td>Categorie</td>
    </tr>

    {% for e in listeEvents %}
        <tr>
            <td>{{ e.dateD | date }}</td>
            <td>{{ e.dateF | date }}</td>
            <td>{{ e.lieu }}</td>
            <td>{{ e.categorie.categorie_name}}</td>
            <td><a href="{{ path('event_delete', {'id': e.id}) }}"> Delete Event</a> </td>
            <td><a href="{{ path('event_update', {'id': e.id}) }}">Update Event</a></td>


        </tr>
    {% endfor %}
</table>